<?xml version="1.0" encoding="utf-8"?>
<scpd
	xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>5</minor>
	</specVersion>
	<actionList>
		<!-- FanLevel Start -->
		<action>
			<name>SetFanLevelStatus</name>
			<argumentList>
				<argument>
					<name>newFanLevelStatus</name>
					<direction>in</direction>
					<relatedStateVariable>FanLevelStatus</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetFanLevelStatus</name>
			<argumentList>
				<argument>
					<name>FanLevelStatusValue</name>
					<direction>out</direction>
					<relatedStateVariable>FanLevelStatus</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- FanLevel End -->
	</actionList>
	<!-- State Variables -->
	<serviceStateTable>
		<!-- Off Button Start -->
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>OffLowerLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>OffUpperLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>OffTarget</name>
			<dataType>i4</dataType>
		</stateVariable>
		<!-- Off Button End -->
		<!-- High Button Start -->
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>HighLowerLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>HighUpperLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>HighTarget</name>
			<dataType>i4</dataType>
		</stateVariable>
		<!-- High Button End -->
		<!-- Med Button Start -->
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>MedLowerLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>MedUpperLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>MedTarget</name>
			<dataType>i4</dataType>
		</stateVariable>
		<!-- Med Button End -->
		<!-- Low Button Start -->
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>LowLowerLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>LowUpperLimit</name>
			<dataType>i4</dataType>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>LowTarget</name>
			<dataType>i4</dataType>
		</stateVariable>
		<!-- Low Button End -->
		<!-- FanLevel Start -->
		<stateVariable>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<name>FanLevelStatus</name>
			<dataType>i4</dataType>
			<setDefaultValue>1</setDefaultValue>
			<defaultValue>0</defaultValue>
		</stateVariable>
		<!-- Fan Level End -->
	</serviceStateTable>
	<!-- stateVariables End -->
</scpd>

